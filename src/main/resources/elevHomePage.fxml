<GridPane hgap="2" prefHeight="400" prefWidth="600" style="-fx-background-color: PEACHPUFF;" vgap="3" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.CatalogVirtual.Controllers.ElevHomePage">
    <TilePane alignment="CENTER" prefHeight="100" prefWidth="600" style="-fx-background-color: BURLYWOOD; -fx-border-style: inset; -fx-border-style: outset; -fx-border-color: BLACK;" GridPane.rowIndex="0">
        <Text id="title-text" text="Bine ati venit!" textAlignment="CENTER"><font>
            <Font name="Bookman Old Style Bold Italic" size="24.0" />
        </font></Text>
      <effect>
         <InnerShadow>
            <color>
               <Color red="1.0" green="0.9870370626449585" blue="0.9870370626449585" />
            </color>
         </InnerShadow>
      </effect>
    </TilePane>
    <HBox alignment="CENTER" spacing="10" GridPane.rowIndex="10">
        <Button fx:id="butonProfesori" onAction="#handleProfesori" prefHeight="40" prefWidth="400" style="-fx-background-color: BURLYWOOD; -fx-border-style: inset; -fx-border-style: outset; -fx-border-color: BLACK;" text="Vizualizare date de contact ale profesorilor"><font>
            <Font name="System Bold" size="12.0" />
        </font>
        </Button>
    </HBox>
    <HBox alignment="CENTER" spacing="10" GridPane.rowIndex="20">
        <Button fx:id="butonSituatieScolara" onAction="#handleSituatieScolara" prefHeight="40" prefWidth="400" style="-fx-background-color: BURLYWOOD; -fx-border-style: inset; -fx-border-style: outset; -fx-border-color: BLACK;" text="Vizualizare Situatie Scolara"><font>
            <Font name="System Bold" size="12.0" />
        </font></Button>
    </HBox>
    <HBox alignment="CENTER" spacing="10" GridPane.rowIndex="30">
        <Button fx:id="butonDeconectare" onAction="#handleDeconectare" prefHeight="40" prefWidth="400" style="-fx-background-color: BURLYWOOD; -fx-border-style: inset; -fx-border-style: outset; -fx-border-color: BLACK;" text="Deconectare"><font>
            <Font name="System Bold" size="12.0" />
        </font></Button>
    </HBox>
