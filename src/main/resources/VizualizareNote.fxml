<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="center" style="-fx-background-color: PEACHPUFF" hgap="10" prefHeight="400" prefWidth="600"
          vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal"
          fx:controller="org.CatalogVirtual.Controllers.Note">
    <GridPane alignment="CENTER" GridPane.rowIndex="1" >
    <TableView fx:id="tableView" style="-fx-border-color: BLACK; -fx-background-color: BURLYWOOD" ></TableView>
    </GridPane>
    <GridPane alignment="CENTER" GridPane.rowIndex="2">
    <HBox alignment="CENTER" spacing="10" >
        <Button fx:id="backButton" prefHeight="40" prefWidth="400" onAction="#handleInapoi" style="-fx-background-color: BURLYWOOD; -fx-border-style: inset; -fx-border-style: outset; -fx-border-color: BLACK;" text="Inapoi"><font>
            <Font name="System Bold" size="12.0" />
        </font></Button>
    </HBox>
</GridPane>
</GridPane>
