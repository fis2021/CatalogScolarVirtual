<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="center" style="-fx-background-color: PEACHPUFF" hgap="10" prefHeight="400" prefWidth="600"
          vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal"
          fx:controller="org.CatalogVirtual.Controllers.Medie">
    <padding>
        <Insets bottom="25.0" left="25.0" right="25.0" top="25.0"/>
    </padding>
    <GridPane GridPane.rowIndex="2" alignment="CENTER"><Text fx:id="medii" GridPane.rowIndex="2" textAlignment="CENTER" ><font>
        <Font name="Bookman Old Style Bold Italic" size="18.0" />
    </font>
    </Text></GridPane>
    <GridPane GridPane.rowIndex="3" alignment="CENTER">
        <Text text="Medie finala:" GridPane.columnIndex="0"><font>
            <Font name="Bookman Old Style Bold Italic" size="18.0" />
        </font></Text>
        <Text fx:id="MedieAnuala" GridPane.columnIndex="1" textAlignment="CENTER" ><font>
        <Font name="Bookman Old Style Bold Italic" size="18.0" />
    </font>
    </Text></GridPane>
    <GridPane alignment="CENTER" GridPane.rowIndex="1">
        <Text text="Medii:" GridPane.rowIndex="1" textAlignment="CENTER" ><font>
            <Font name="Bookman Old Style Bold Italic" size="24.0" />
        </font>
        </Text></GridPane>
    <GridPane GridPane.rowIndex="4" alignment="CENTER">
        <HBox alignment="CENTER" spacing="10" GridPane.rowIndex="50">
            <Button fx:id="backButton" prefHeight="40" prefWidth="400" style="-fx-background-color: BURLYWOOD; -fx-border-style: inset; -fx-border-style: outset; -fx-border-color: BLACK;" onAction="#handleInapoi" text="Inapoi"><font>
                <Font name="System Bold" size="12.0" />
            </font></Button>
        </HBox> </GridPane>
</GridPane>

