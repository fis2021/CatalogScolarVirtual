<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.paint.Color?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="center" style="-fx-background-color: PEACHPUFF" hgap="10" prefHeight="400" prefWidth="600"
          vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal"
          fx:controller="org.CatalogVirtual.Controllers.VizualizareProfesori">
    <Text id="title-text" text="Profesori" style="-fx-alignment: CENTER" GridPane.rowIndex="1"><font><Font name="Bookman Old Style Bold Italic" size="24.0" />
    </font></Text>
    <TableView fx:id="tableView" GridPane.rowIndex="2" style="-fx-border-color: BLACK; -fx-background-color: BURLYWOOD" ></TableView>
    <HBox alignment="CENTER" spacing="10" GridPane.rowIndex="3">
        <Button fx:id="backButton" prefHeight="40" prefWidth="400" style="-fx-background-color: BURLYWOOD; -fx-border-style: inset; -fx-border-style: outset; -fx-border-color: BLACK;" onAction="#handleBackAction" text="Inapoi la pagina principala"><font>
            <Font name="System Bold" size="12.0" />
        </font></Button>
    </HBox>
</GridPane>
