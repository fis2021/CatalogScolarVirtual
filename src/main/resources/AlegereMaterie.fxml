<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.paint.Color?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="center" style="-fx-background-color: PEACHPUFF" hgap="10" prefHeight="400" prefWidth="600"
          vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal"
          fx:controller="org.CatalogVirtual.Controllers.AlegereMaterie">
    <TilePane alignment="CENTER" prefHeight="100" prefWidth="600" style="-fx-background-color: BURLYWOOD; -fx-border-style: inset; -fx-border-style: outset; -fx-border-color: BLACK;" GridPane.rowIndex="0">
        <Text id="title-text" text="Alegeti materia" textAlignment="CENTER"><font>
            <Font name="Bookman Old Style Bold Italic" size="24.0" />
        </font></Text>
        <effect>
            <InnerShadow>
                <color>
                    <Color red="1.0" green="0.9870370626449585" blue="0.9870370626449585" />
                </color>
            </InnerShadow>
        </effect>
    </TilePane>

    <GridPane GridPane.rowIndex="1" alignment="CENTER">
    <ChoiceBox fx:id="Materie" prefWidth="300"  GridPane.rowIndex="10" style="-fx-alignment: center">
    </ChoiceBox></GridPane>
    <GridPane GridPane.rowIndex="2" alignment="CENTER">
    <HBox alignment="bottom_right" spacing="10"  GridPane.rowIndex="20">
        <Button fx:id="buttonAlegereMaterie" onAction="#handleAlegere" text="Selectare"><font>
            <Font name="System Bold" size="12.0" />
        </font></Button>
    </HBox>
    </GridPane>

</GridPane>
